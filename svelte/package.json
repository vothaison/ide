        "svelte-check": "^3.3.2",
        "svelte-preprocess": "^5.0.3",
        "tailwindcss": "^3.3.2",
        "tslib": "^2.5.0",
        "typescript": "^5.0.2",
        "vite": "^4.3.2"
    },
    "type": "module",
    "dependencies": {
        "@azure/msal-browser": "^2.28.0",
        "@revolist/svelte-datagrid": "^3.2.16",
        "@tailwindcss/forms": "^0.5.3",
        "ajv": "^8.11.0",
        "axios": "^1.4.0",
        "currency.js": "^2.0.4",
        "fast-xml-parser": "^4.2.5",
        "imask": "^6.6.1",
        "lodash": "^4.17.21",
        "luxon": "^3.3.0",
